{
  "packageName": "type-registry",
  "description": "Centrally Managed Types for the Ecommerce Project",
  "schemas": {
    "product": {
      "version": "1.0.0",
      "schema": {
        "$id": "product",
        "type": "object",
        "properties": {
          "id": {
            "type": "ProductId"
          },
          "name": {
            "type": "string"
          },
          "price": {
            "type": "number"
          },
          "tag": {
            "type": "string"
          }
        }
      }
    },
    "user": {
      "version": "1.0.0",
      "schema": {
        "$id": "user",
        "type": "object",
        "properties": {
          "id": {
            "type": "UserId"
          },
          "name": {
            "type": "string"
          },
          "email": {
            "type": "string"
          },
          "address": {
            "type": "string"
          },
          "phone": {
            "type": "string"
          }
        }
      }
    },
    "orders": {
      "version": "1.0.0",
      "schema": {
        "$id": "user",
        "type": "object",
        "properties": {
          "id": {
            "type": "OrderId"
          },
          "price": {
            "type": "number"
          }
        }
      }
    },
    "vendors": {
      "version": "1.0.0",
      "schema": {
        "$id": "user",
        "type": "object",
        "properties": {
          "id": {
            "type": "VendorId"
          },
          "name": {
            "type": "string"
          }
        }
      }
    }
  },
  "types": {
    "ProductId": "integer",
    "UserId": "string",
    "VendorId": "string",
    "OrderId": "string"
  }
}